{
    "name": "@exabyte-io/wode.js",
    "version": "0.0.0",
    "description": "WOrkflow DEfinitions",
    "scripts": {
        "test": "nyc --reporter=text mocha --recursive --bail --require @babel/register/lib --require tests/setup.js tests",
        "lint": "eslint src tests && prettier --write src tests",
        "lint:fix": "eslint --fix --cache src tests && prettier --write src tests",
        "build:workflows": "node build_workflows.js",
        "transpile": "babel --out-dir dist src; npm run build:workflows",
        "postinstall": "npm run transpile",
        "prettier": "prettier --check src tests",
        "prepare": "husky install || exit 0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Exabyte-io/wode.js.git"
    },
    "main": "dist/index.js",
    "files": [
        "/assets",
        "/dist",
        "/src",
        ".babelrc",
        "build_workflows.js"
    ],
    "author": "Exabyte Inc.",
    "bugs": {
        "url": "https://github.com/Exabyte-io/wode.js/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Exabyte-io/wode.js",
    "dependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.7",
        "@babel/eslint-parser": "^7.26.5",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.26.7",
        "@babel/preset-react": "^7.26.3",
        "@babel/register": "^7.25.9",
        "@babel/runtime-corejs3": "^7.26.7",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.21",
        "mixwith": "^0.1.1",
        "underscore": "^1.13.7",
        "underscore.string": "^3.3.6"
    },
    "devDependencies": {
        "@exabyte-io/ade.js": "2024.12.5-0",
        "@exabyte-io/application-flavors.js": "2024.12.5-0",
        "@exabyte-io/eslint-config": "^2025.1.15-0",
        "@exabyte-io/ide.js": "2024.3.26-0",
        "@exabyte-io/mode.js": "2024.4.28-0",
        "@mat3ra/code": "2025.1.27-0",
        "@mat3ra/esse": "git+https://github.com/Exabyte-io/esse.git#049de31c74d03e1184a6c7de8d0e55e10b44c0d2",
        "@mat3ra/made": "2025.1.23-0",
        "chai": "^4.5.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.10.0",
        "eslint-import-resolver-exports": "^1.0.0-beta.5",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.6.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.5.0",
        "mocha": "^9.2.2",
        "nyc": "^15.1.0",
        "prettier": "^2.8.8"
    },
    "peerDependencies": {
        "@exabyte-io/ade.js": "*",
        "@exabyte-io/ide.js": "*",
        "@exabyte-io/mode.js": "*",
        "@mat3ra/code": "*",
        "@mat3ra/esse": "*",
        "@mat3ra/made": "*"
    },
    "engines": {
        "node": ">=20"
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,css}": "prettier --write"
    }
}
